<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<BorderPane maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="387.0" prefWidth="611.0"
            xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.getaji.bmshashwatcher.controller.PreferenceDialogController">
    <center>
        <TabPane prefHeight="200.0" prefWidth="604.0" tabClosingPolicy="UNAVAILABLE" BorderPane.alignment="CENTER">
            <tabs>
                <Tab text="楽曲DB">
                    <content>
                        <AnchorPane minWidth="200.0">
                            <children>
                                <VBox layoutX="111.0" layoutY="63.0" spacing="10.0" AnchorPane.bottomAnchor="0.0"
                                      AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                                      AnchorPane.topAnchor="0.0">
                                    <children>
                                        <CheckBox fx:id="checkboxUseBeatorajaDB" mnemonicParsing="false"
                                                  text="beatorajaの楽曲データベースを利用する"/>
                                        <BorderPane>
                                            <center>
                                                <TextField fx:id="fieldBeatorajaPath" BorderPane.alignment="CENTER"/>
                                            </center>
                                            <right>
                                                <Button mnemonicParsing="false" onAction="#onActionChooseBeatorajaPath"
                                                        text="選択..." BorderPane.alignment="CENTER"/>
                                            </right>
                                        </BorderPane>
                                        <Label fx:id="labelErrorBeatorajaPath" style="-fx-background-color: #FFCCCC;"
                                               text="Label">
                                            <VBox.margin>
                                                <Insets bottom="10.0"/>
                                            </VBox.margin>
                                            <padding>
                                                <Insets bottom="2.0" left="2.0" right="2.0" top="2.0"/>
                                            </padding>
                                        </Label>
                                        <CheckBox fx:id="checkboxUseLR2DB" mnemonicParsing="false"
                                                  text="LR2の楽曲データベースを利用する"/>
                                        <BorderPane>
                                            <center>
                                                <TextField fx:id="fieldLR2Path" BorderPane.alignment="CENTER"/>
                                            </center>
                                            <right>
                                                <Button mnemonicParsing="false" onAction="#onActionChooseLR2Path"
                                                        text="選択..." BorderPane.alignment="CENTER"/>
                                            </right>
                                        </BorderPane>
                                        <Label fx:id="labelErrorLR2Path" style="-fx-background-color: #FFCCCC;"
                                               text="Label">
                                            <padding>
                                                <Insets bottom="2.0" left="2.0" right="2.0" top="2.0"/>
                                            </padding>
                                        </Label>
                                    </children>
                                    <padding>
                                        <Insets bottom="20.0" left="10.0" right="10.0" top="20.0"/>
                                    </padding>
                                </VBox>
                            </children>
                        </AnchorPane>
                    </content>
                </Tab>
                <Tab text="Webサービス">
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                            <children>
                                <BorderPane layoutX="102.0" layoutY="31.0" prefHeight="200.0" prefWidth="200.0"
                                            AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                                            AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                    <bottom>
                                        <HBox spacing="4.0" BorderPane.alignment="CENTER">
                                            <children>
                                                <Button mnemonicParsing="false" onAction="#onActionAddWebService"
                                                        text="追加">
                                                    <font>
                                                        <Font size="13.0"/>
                                                    </font>
                                                </Button>
                                                <Button mnemonicParsing="false" onAction="#onActionEditWebService"
                                                        text="編集">
                                                    <font>
                                                        <Font size="13.0"/>
                                                    </font>
                                                </Button>
                                                <Button mnemonicParsing="false" onAction="#onActionRemoveWebService"
                                                        text="削除">
                                                    <font>
                                                        <Font size="13.0"/>
                                                    </font>
                                                </Button>
                                                <Button mnemonicParsing="false"
                                                        onAction="#onActionRestoreDefaultWebService" text="デフォルトに戻す">
                                                    <font>
                                                        <Font size="13.0"/>
                                                    </font>
                                                </Button>
                                            </children>
                                            <BorderPane.margin>
                                                <Insets top="10.0"/>
                                            </BorderPane.margin>
                                        </HBox>
                                    </bottom>
                                    <center>
                                        <BorderPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
                                            <center>
                                                <TableView fx:id="tableWebService" onMouseClicked="#onClickWebService"
                                                           prefHeight="200.0" prefWidth="200.0"
                                                           BorderPane.alignment="CENTER">
                                                    <columns>
                                                        <TableColumn fx:id="columnWebServiceName" prefWidth="105.0"
                                                                     text="名前"/>
                                                        <TableColumn fx:id="columnWebServiceMD5Pattern" minWidth="0.0"
                                                                     prefWidth="220.0" text="MD5 URL"/>
                                                        <TableColumn fx:id="columnWebServiceSHA256Pattern"
                                                                     prefWidth="220.0" text="SHA-256 URL"/>
                                                    </columns>
                                                </TableView>
                                            </center>
                                            <right>
                                                <VBox alignment="CENTER" spacing="2.0" BorderPane.alignment="CENTER">
                                                    <children>
                                                        <Button mnemonicParsing="false"
                                                                onAction="#onActionMoveUpWebService" text="▲"
                                                                textAlignment="CENTER">
                                                            <font>
                                                                <Font size="10.0"/>
                                                            </font>
                                                            <padding>
                                                                <Insets bottom="10.0" left="6.0" right="6.0"
                                                                        top="10.0"/>
                                                            </padding>
                                                        </Button>
                                                        <Button mnemonicParsing="false"
                                                                onAction="#onActionMoveDownWebService" text="▼"
                                                                textAlignment="CENTER">
                                                            <font>
                                                                <Font size="10.0"/>
                                                            </font>
                                                            <padding>
                                                                <Insets bottom="10.0" left="6.0" right="6.0"
                                                                        top="10.0"/>
                                                            </padding>
                                                            <VBox.margin>
                                                                <Insets/>
                                                            </VBox.margin>
                                                        </Button>
                                                    </children>
                                                    <padding>
                                                        <Insets left="4.0"/>
                                                    </padding>
                                                </VBox>
                                            </right>
                                        </BorderPane>
                                    </center>
                                    <padding>
                                        <Insets bottom="8.0" left="8.0" right="8.0" top="8.0"/>
                                    </padding>
                                </BorderPane>
                            </children>
                        </AnchorPane>
                    </content>
                </Tab>
            </tabs>
        </TabPane>
    </center>
</BorderPane>
